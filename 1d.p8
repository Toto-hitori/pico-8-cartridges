local player_x
local player_y
function _init()

	player_x=14
	player_y=64


end



function _update()

	if btn(1) then

		player_x+=1
	end
	if btn(0) then
		player_x-=1

	end
	if btnp(4) then
		sfx(0)
	end
	if btn(3) then

		player_y+=1
	end
	if btn(2) then
		player_y-=1
		sfx(1)
	end
end

function _draw()
	cls()
	--circ(player_x,player_y,10,15)
	--pset(10,100,15)
	--rectfill(22,25,30,30,10)
	pal(2,4)
	spr(1,player_x,player_y)
	
end 

