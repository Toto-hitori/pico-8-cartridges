pico-8 cartridge // http://www.pico-8.com
version 33
__lua__
function _init()
	cookies = 0
	start = time()
	cps = 0
	clickers = 0
	pressed = false
end

function _update()
	if(btn(4))then
	 if( not pressed)then
			pressed = true 
			cookies += 1
		end
	elseif(btn(5) and cookies > clickers*5)then
		if(not pressed)then
			pressed = true
			cookies -= clickers*5
			cps += 1
		end
	else
		pressed = false
	end
	cookies += 1/30*cps
end
function _draw()
	cls()
	map(0,0)
	print(cps,0,0)
	print(flr(cookies),64,64)
end
