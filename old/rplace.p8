function _init()
	board = {}
	old = -1
	game = "menu"
	initialize_board()
	update_board()
	init_tools()
	setup_menu()
	poke(0x5f80+125,0)
end

function _update()
if( game == "menu") then
		update_menu()
 elseif(game == "view") then
 	update_board()
 elseif(game == "edit") then
 	update_board()
 	update_tools()
 end
end

function _draw()
 cls()
 if( game == "menu") then
 	draw_menu()
 elseif(game == "view") then
 	draw_board()
 elseif(game == "edit") then
 	draw_tools()
 end
//print(x,7)
//print(peek(0x5f80+125))
end
