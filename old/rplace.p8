	rect(64-58+60*selected,69+32+up_size,28+64+60*selected-30,69+20+32+up_size,1)
	local logo_y = 10
	sspr(9*8,0,8,8,32,26-logo_y,24,16)// a
	sspr(10*8,0,8*3,8,60,33-logo_y,50,50/3) // nice
	sspr(8*8,8,8*6,8,12,44-logo_y,120,20) // place
