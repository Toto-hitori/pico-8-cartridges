function init_tools()
	active_color = 0
	input_delay = time()
	selected_y = 110
	selected_offset = 2
	selected_size = 10
	x = 9
	y = 114
	size = 8
	dist = 7
	zoom = 0
	speed = 3
	selected_cell = {
		x = 0,
		y = 0
	}
	curso = {
		x = 63,
		y = 47,
		sprite = 2
	}
	selected_pixel = {
		x = 0,
		y = 0
	}
	transfer_ready = false
end
function update_tools()
	if(zoom == 0) then
		update_zoom0()
	elseif(zoom == 1) then
		update_draw()
	end
	
end
function draw_tools()
	if(zoom == 0) then
		draw_zoom0()
	elseif(zoom == 1) then
		draw_draw()
	end
	
end

function update_zoom0()
	if(btn(üÖæÔ∏è)and time() - input_delay > 0.25) then
		game = "menu"
		input_delay = time()
	end
	if(time() - input_delay > 0.15)then
		if(btn(‚û°Ô∏è)) then
			selected_cell.x += 1
			input_delay = time()
		elseif(btn(‚¨ÖÔ∏è))then
			selected_cell.x -= 1
			input_delay = time()
		elseif(btn(‚¨áÔ∏è))then
			selected_cell.y += 1
			input_delay = time()
		elseif(btn(‚¨ÜÔ∏è))then
			selected_cell.y -= 1
			input_delay = time()
		end
	end
	if(selected_cell.x > 3) then
		selected_cell.x = 3
	elseif(selected_cell.x < 0) then
		selected_cell.x = 0
	end
	if(selected_cell.y > 3) then
		selected_cell.y = 3
	elseif( selected_cell.y < 0 ) then
		selected_cell.y = 0
	end
	if(time() - input_delay > 0.25) then
		if(btn(‚ùé)) then
			zoom += 1
			input_delay = time()
		end
	end
end
function draw_zoom0()
	draw_board()
	cell_size = 32
	
	rect(selected_cell.x*cell_size,selected_cell.y*cell_size,selected_cell.x*cell_size+cell_size,selected_cell.y*cell_size+cell_size,2)
end
function draw_zoom1()
	
	for i = 0+selected_cell.x*32,selected_cell.x*32+31 do
 	for j = 0+selected_cell.y*32,selected_cell.y*32+31 do
 		rectfill(i*3+16-selected_cell.x*96,j*3+5-selected_cell.y*96,i*3+16+2-selected_cell.x*96,j*3+5+2-selected_cell.y*96,board[j+1][i+1])
 	end
 end
	cell_size = 3
	local sel_x = selected_pixel.x*cell_size
	local sel_y = selected_pixel.y*cell_size
	if(curso.sprite == 2) then
		rect(sel_x+18,sel_y+4,sel_x+cell_size+18,sel_y+cell_size+4,active_color)
	end
	
end
function update_draw()
	if(btn(üÖæÔ∏è) and time() - input_delay > 0.25)   then
		zoom -= 1
		input_delay = time()
	end
	x = 9
	y = 114
	size = 8
	dist = 7
	if btn(‚¨ÖÔ∏è) then
		curso.x -= speed
	elseif btn(‚û°Ô∏è) then
		curso.x += speed
	end
	if btn(‚¨áÔ∏è) then
		curso.y += speed
	elseif btn(‚¨ÜÔ∏è) then
		curso.y -= speed
	end
	if(active_color < 0) then
		active_color = 15
	elseif(active_color  > 15) then
		active_color = 0
	end
	selected_y = lerp(selected_y,110-selected_offset,0.5)
	
	if( active_color > 3) then
	 x -= (active_color-3)*16
	end
	if( active_color > 15-4) then
		x = 4-8*15
	end
	update_curso()

end

function update_curso()
	if(curso.x > 14 and curso.x < 13+98 and curso.y > -2 and curso.y < 5+90)then
		selected_pixel.x = flr((curso.x-12)/3-2)
		selected_pixel.y = flr((curso.y-4)/3+2)
		curso.sprite = 2
		speed = 2
		if(btn(‚ùé) and time() - input_delay > 0.15 and peek(0x5f80+125) == 0) then
				local pixel_y = selected_pixel.y+1 +selected_cell.y*32
				local pixel_x = selected_pixel.x+2 +selected_cell.x*32
				board[pixel_y][pixel_x] = active_color
				
				poke(0x5f80+122,pixel_x) // put x coord
				poke(0x5f80+123,pixel_y) // put y coord
				poke(0x5f80+124,active_color) //put color
				poke(0x5f80+125,1)//check if done
			end
	elseif(curso.x > 6 and curso.x < 13+98 and curso.y > 17+90 and curso.y < 120)then
		curso.sprite = 4
		speed = 3
		selected_color = flr((curso.x -x+size-dist/2)/(dist+size))
		if(btn(‚ùé) and time() - input_delay > 0.15)then
			active_color = selected_color
			input_delay = time()
		end
		//x+i*(size+dist),y,x+size+i*(size+dist),y+size,i)
			//rect(x-1+i*(size+dist),y-1,x+size+1+i*(size+dist),y+size+1,0)
	else
		curso.sprite = 3
		speed = 3
	end
	if(curso.x <-2) then
		curso.x = -2
	elseif( curso.x > 117) then
		curso.x = 117
	end
	if(curso.y <0) then
		curso.y = 0
	elseif( curso.y > 122) then
		curso.y = 122
	end
end
function draw_draw()
	rectfill(0,0,127,4,8)
	rectfill(0,0,15,127,8)
	rectfill(127-16,0,127,127,8)
	draw_zoom1()
	rectfill(0,100,127,127,8)
	rect(15,4,127-15,100,2)
	for i = 0,15 do
		if(i == active_color) then
			local selected_x = x+i*(size+dist)-2
			//local selected_y = y-selected_offset
			rectfill(selected_x,selected_y-1,selected_x+selected_size+2,selected_y+selected_size+1,i)
			rect(selected_x-1,selected_y-selected_offset,selected_x+selected_size+2+1,selected_y+selected_size-selected_offset+4,13)
		else
			rectfill(x+i*(size+dist),y,x+size+i*(size+dist),y+size,i)
			rect(x-1+i*(size+dist),y-1,x+size+1+i*(size+dist),y+size+1,2)
		end
	end
	if(active_color > 3) then
		rectfill(0,106,15,128,8)
		spr(1,4,y,1,1,true,true)
	end
	
	if(active_color < 11) then
		rectfill(113,106,128,128,8)
		spr(1,116,y)
	end
	spr(curso.sprite,curso.x+3,curso.y-3)
	print("x",2,15-6,1)
	print(selected_pixel.x+1+selected_cell.x*32,2,15,1)
	print("y",2,30-6,1)
	print(selected_pixel.y+selected_cell.y*32,2,30,1)
end
