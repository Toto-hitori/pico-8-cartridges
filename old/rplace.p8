function initialize_board()
	for i = 1,128 do
	 row = {}
		for j = 1,128 do
			row[j] = 7
		end
		board[i] = row
	end
end
function update_board()
	if(peek(0x5f80+125) == 0)then
		pin =  peek(0x5f80+126)
		pin2 =  peek(0x5f80+127)
		if( pin2 == 0) then
			local row1 = {}
		 for i = 1,64 do
		  row1[i] = flr(peek(0x5f80+i)/16)
			  row1[64+i] = peek(0x5f80+i)%16
		 end
		 board[pin+1] = row1
		 old = pin
		 poke(0x5f80+127,1)
		end
	end
		
end
function draw_board()
	if(btn(üÖæÔ∏è)and time() - input_delay > 0.25) then
		game = "menu"
		input_delay = time()
	end
	for i = 0,127 do
 	for j = 0,127 do
 		pset(i,j,board[j+1][i+1])
 	end
 end
	
end
