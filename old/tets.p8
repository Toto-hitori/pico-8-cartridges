pico-8 cartridge // http://www.pico-8.com
version 33
__lua__
function _init()
 make_player()
end
function _update()
 move_player()
 apply_gravity()
end
function _draw()
 cls() --clear screen
 draw_player()
end
function make_player()
 airborn = false
 px=10
 py=70
 psprite=1
end
function apply_gravity()
	if(py < 97) py +=1 --gravity
	if(py > 96) airborn = false
end
function move_player()
 if (btn(0)) px-=1 --left
 if (btn(1)) px+=1 --right
 if (btn(2)) jump() --up
 if (btn(3)) py+=1 --down
end
function jump()
	if(not airborn) then 
		py -=2
		airborn = true
	end
end
function draw_player()
 spr(psprite,px,py)
end
