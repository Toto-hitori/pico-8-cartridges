pico-8 cartridge // http://www.pico-8.com
version 33
__lua__
function _init()
 make_player()
 lines ={
		initial_x = 60,
		initial_y = 60,
		length = 60,
		end_x = 0,
		end_y = 0,
		angle = 3.14/3
		}
end
function _update()
	if(player.hanging)then
		player.px = lines.initial_x-4
		player.py = lines.initial_y-5
		end
 move_player()
 apply_gravity()
	
end
function _draw()
 
 cls() --clear screen
 draw_player()
 map(0,0)
 if(player.rope) throw_rope()
 print(player.speedx)
end
function make_player()
 player = {
 	speedx = 0,
 	speedy = 0,
 	airborn = false,
 	px=10,
 	py=70,
 	psprite=1,
 	dashbool = true,
 	rope = false,
 	hanging = false
 }
end
function throw_rope()
		
		compute_end()
		line(lines.initial_x,lines.initial_y,lines.end_x,lines.end_y)

end
function compute_end()
		lines.initial_x = player.px+4
		lines.initial_y = player.py+4
		local endx = -sin(lines.angle)*lines.length+lines.initial_x
		local endy = -cos(lines.angle)*lines.length+lines.initial_y
	
		if(endy <= 53 and not player.hanging)then
			lines.end_y = 53
			tan = -sin(lines.angle)/cos(lines.angle)
			lines.end_x = lines.initial_x+53*tan
			player.hanging = true
		elseif(not player.hanging)then
			lines.end_x = endx 
			lines.end_y = endy
	end
	if(lines.end_y - lines.initial_y)/sin(lines.angle)>lines.length then
				lines.initial_y =lines.end_y-(sin(lines.angle)*lines.length)
				--lines.initial_x = lines.end_x+(cos(lines.angle)*lines.length)
				--player.py = lines.initial_y
				end
	if(lines.end_x - lines.initial_x)/sin(lines.angle)>lines.length then
				lines.initial_x =lines.end_x-(sin(lines.angle)*lines.length)
				--lines.initial_x = lines.end_x+(cos(lines.angle)*lines.length)
				player.px = lines.initial_x
				player.py = lines.initial_y
				end
end
function apply_gravity()
	if(player.py < 97 and player.speedy < 3) then
		player.speedy +=0.4 --gravity
		end
	if(player.py > 96)then
	 player.airborn = false
		player.dashbool = true
	 player.py = 96
		player.speedy = 0
	 end
end
function move_player()
 if (btn(0))then
  walk(0)--left
 elseif (btn(1)) then
 	walk(1)--right 
 else player.speedx = 0 
 end
 if (btn(2)) jump() --up
 if (btn(3) and lines.length >0) then
 	lines.length -= 1
 	end
 if (btn(5))then
  player.rope = true
 else
 	player.rope = false
 	player.hanging = false
 	lines.length = 60
 end
	player.py += player.speedy
 player.px += player.speedx
end
function walk(dir)
	if(dir ==0) then
		if(player.speedx>-1.2)then
		 player.speedx -=0.6
		end
	else 
		if(player.speedx < 1.2)then
			player.speedx +=0.6
			
		end
	end
end		
function dash()
	if(player.dashbool and player.airborn)
	then
		player.dashbool = false
		if(player.speedx > 0) then
			player.speedx += 20
			player.speedy += 10
		else
			player.speedx -= 20
			player.speedy += 10
		end
	end
end
function jump()
	if(not player.airborn) 
	then 
		player.speedy -=4
		player.airborn = true
end
end
function draw_player()
 spr(player.psprite,player.px,player.py)
end
